import java.util.Scanner;
public class Main {

	public static void main(String[] args) {
		int a,b,c;
		Scanner sc = new Scanner(System.in);
		a = sc.nextInt();
		b = sc.nextInt();
		c = sc.nextInt();
	      if((b < a && a < c) || (c < a && a < b) || (a==b)) {
	          System.out.print(a);
	       }
	       else if ((a < b && b < c)  || (c < b && b < a)) {
	          System.out.print(b);
	       }
	       else {
	          System.out.print(c);
	       }
	}

}
