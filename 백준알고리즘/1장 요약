import java.util.Scanner;
public class Main {

	public static void main(String[] args) 
	{
		int a,b,c,d,e,result;
		Scanner sc = new Scanner(System.in);
		a = sc.nextInt();
		b = sc.nextInt();
		c = a * (b % 10); 
		d = a * ((b / 10) % 10); 
		e = a * (((b / 10) / 10) % 10); 
		result = a*b;
		System.out.println(c);
		System.out.println(d);
		System.out.println(e);
		System.out.println(result);
	}

}

