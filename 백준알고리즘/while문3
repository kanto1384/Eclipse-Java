import java.util.Scanner;

public class Main{
	public static void main(String args[])  {
		
	Scanner sc = new Scanner(System.in);
	int value = sc.nextInt();
	int a[] = new int[2];
	int cnt = 0;
	int x =0;
	int y =0;
	int z =0;
	x=value/10;
	y=value%10;
	a[0] = x;
	a[1] = y;
	while(true){
		if((a[0]==x&&a[1]==y)&&!(cnt==0)) {
			System.out.print(cnt);
			return;
		}else {
			z=a[0]+a[1];
			if(z>=10) {
				z=z-10;
			}
			a[0]=a[1];
			a[1]=z;
			cnt++;
		}
	}
}
}
