import java.util.Scanner;
class Main {
    public static void main(String args[]) 
    {
    	int T,a,b,c;
    	Scanner sc = new Scanner(System.in);
    	T = sc.nextInt();
    	for(int i=0;i<T;i++)
    	{
    		a = sc.nextInt();
    		b = sc.nextInt();
    		c = a+b;
    		System.out.println("Case #"+ (i+1)+": " + a + " + " + b +" = "+c);
    	}
}
}
