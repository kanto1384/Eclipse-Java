package 백준알고리즘2;

import java.util.*;
public class NumberSum {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int a= sc.nextInt();
		String b = sc.next();
		int c=0;
		for(int i=0;i<a;i++)
		{
			c += b.charAt(i)-48; // 아스키코드로 인지하기때문에 -48을 넣어 정수형태로 
		}
		System.out.println(c);
	}

}
