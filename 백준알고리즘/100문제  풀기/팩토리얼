import java.util.*;
public class Factorial {
	 
	    public static void main(String[] args){
	    	Scanner sc = new Scanner(System.in);
	    	int n = sc.nextInt();
	        int answer = 1;
	        if (n > 1) {
	            // 1이 아니면
	            for (int i = 2; i <= n; i++) {
	                answer *= i;
	            }
	        }
	        System.out.println(answer);
	 
	    }
	 
	}

