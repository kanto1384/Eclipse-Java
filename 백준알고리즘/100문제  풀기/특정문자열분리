package 백준알고리즘2;

import java.util.*;
public class StringSub {

	public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        String St = sc.next();
        sc.close();
         
        for (int i = 0; i < St.length(); ++i) {
            System.out.print(St.charAt(i));
            if (i % 10 == 9) {
                System.out.println();
	}

}
	}
}
