package text;
public class Bills 
{
  public static void main(String[] args) 
  {
    // 변수 생성
    int sejongCnt = 3;   // 만원권 수
    int leeleeCnt = 4;   // 오천원권 수
    int leehwangCnt = 7; // 천원권 수
    
    // 계산
    int total = 0;
    total += 10000 * sejongCnt;
    total += 5000 * leeleeCnt;
    total += 1000 * leehwangCnt;

    System.out.println(total + "원");
  }
}
